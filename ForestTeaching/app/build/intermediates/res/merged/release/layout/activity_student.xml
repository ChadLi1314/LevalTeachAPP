    android:id="@+id/activity_student"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ldp.com.activity.StudentActivity">

    <include
        android:id="@+id/sss_id"
        layout="@layout/titel_main"
